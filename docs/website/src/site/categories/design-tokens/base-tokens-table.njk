<section class="ds-tokens">
<h2 class="ds-tokens__heading">Color</h2>
<table class="ds-table"><thead>
 <tr>
   <th>Example</th>
   <th>Token title</th>
   <th>Token</th>
 </tr></thead>
 <tbody>
 {% for color in tokens.base.color.palette -%}
   {% for palette in color -%}
     {% for colorPrimary in palette -%}
       {% for colorVariantValue in colorPrimary -%}
         {% if colorVariantValue.value %}
   <tr>
   <td>
     <div class="ds-tokens__color" style="background-color: {{ colorVariantValue.value }}"></div>
     <code>{{ colorVariantValue.value }}</code>
   </td>
   <td class="ds-tokens__description">{{ colorVariantValue.description | tokenDescription }}</td>
   <td>{% if colorVariantValue.description %}<code>{{ colorVariantValue.description | tokenName }}{% endif %}</code></td>
   </tr>
        {% endif %}
       {% endfor -%}
     {% endfor -%}
   {% endfor -%}
 {% endfor -%}
 </tbody>
</table>
</section>

<section class="ds-tokens">
<h2 class="ds-tokens__heading">Text</h2>
<table class="ds-table">
<thead><tr>
    <th>Example text value</th>
    <th>Token title</th>
    <th>Token</th>
  </tr></thead>
<tbody>
  {% for text in tokens.base.text -%}
    {% if text[0] == 'typeset' %}
    {% for textType in text -%}
      {% for textProperties in textType -%}
        {% for property in textProperties -%}
          {% for propertyValue in property -%}
            {% if propertyValue[0] | notContainsIconValue %}
              {% for textType in propertyValue -%}
                {% if textType.value %}
  <tr>
  <td>
    {% if textType.type != 'paragraphSpacing' %}
    <p class="ds-tokens__text"
       style="{% if textType.type == 'fontFamilies' %}font-family: {{ textType.value }}, sans-serif;{% endif %}{% if textType.type == 'fontWeights' %}font-weight: {{ textType.value | fontWeight }};{% endif %}{% if textType.type == 'textCase' %}text-transform: {{ textType.value }};{% endif %}
       {% if textType.type == 'textDecoration' %}text-decoration: {{ textType.value }};{% endif %}{% if textType.type == 'fontSizes' %}font-size: {{ textType.value }}px;{% endif %}{% if textType.type == 'lineHeights' %}display: none;{% endif %}">Lorem ipsum</p>
    {% endif %}
    <code>{% if textType.type != 'paragraphSpacing' %}{{ textType.value | fontWeight }}{% if textType.type == 'fontSizes' or textType.type == 'lineHeights' %}px{% endif %}{% endif %}{% if textType.type == 'paragraphSpacing' %}{{ textType.value }}{% if textType.value != 0 %}px{% endif %}{% endif %}
    </code>
  </td>
  <td class="ds-tokens__description">{{ textType.description | tokenDescription }}</td>
  <td>{% if textType.description %}<code>{{ textType.description | tokenName }}</code>{% endif %}</td>
  </tr>
                  {% endif %}
                {% endfor -%}
              {% endif %}
            {% endfor -%}
          {% endfor -%}
        {% endfor -%}
      {% endfor -%}
    {% endif %}
  {% endfor -%}
</tbody>
</table>
</section>

<section class="ds-tokens">
<h2 class="ds-tokens__heading">Border</h2>
<table class="ds-table"><thead>
<tr>
  <th>Example</th>
  <th>Token title</th>
  <th>Token</th>
</tr></thead>
<tbody>
  {% for border in tokens.base.border -%}
    {% for property in border -%}
      {% for type in property -%}
        {% for widthValue in type -%}
          {% if widthValue.value %}
          {% if widthValue.type == 'borderWidth' %}
  <tr>
    <td>
      <div class="ds-tokens__border-example">
      <div class="ds-tokens__border" style="border-width: {{ widthValue.value }}px"></div>
      <code>{{ widthValue.value }}{% if widthValue.value != 0 %}px{% endif %}</code>
      </div>
    </td>
    <td class="ds-tokens__description">{{ widthValue.description | tokenDescription }}</td>
    <td>{% if widthValue.description %}<code>{{ widthValue.description | tokenName }}</code>{% endif %}</td>
  </tr>
          {% endif %}
          {% if widthValue.type == 'borderRadius' %}
  <tr>
    <td>
      <div class="ds-tokens__border-example">
      <div class="ds-tokens__border" style="border-radius: {{ widthValue.value }}{% if widthValue.value | hasNoUnit %}px{% endif %}"></div>
      <code>{{ widthValue.value }}{% if widthValue.value | hasNoUnit %}px{% endif %}</code>
      </div>
    </td>
    <td class="ds-tokens__description">{{ widthValue.description | tokenDescription }}</td>
    <td>{% if widthValue.description %}<code>{{ widthValue.description | tokenName }}</code>{% endif %}</td>
  </tr>
          {% endif %}
          {% endif %}
        {% endfor -%}
      {% endfor -%}
    {% endfor -%}
  {% endfor -%}
</tbody>
</table>
</section>

<section class="ds-tokens">
<h2 class="ds-tokens__heading">Spacing</h2>
<table class="ds-table"><thead>
  <tr>
    <th>Example</th>
    <th>Token title</th>
    <th>Token</th>
  </tr></thead>
  <tbody>
  {% for type in tokens.base.space -%}
    {% for property in type -%}
      {% if property.value %}
  <tr>
    <td>
      <div class="ds-tokens__space-example">
      <div class="ds-tokens__space" style="width: {{ property.value }}px; height: {{ property.value }}px"></div>
      <code>{{ property.value }}{% if property.value != 0 %}px{% endif %}</code>
      </div>
    </td>
    <td class="ds-tokens__description">{{ property.description | tokenDescription }}</td>
    <td><code>{{ property.description | tokenName }}</code></td>
  </tr>
      {% endif %}
    {% endfor -%}
  {% endfor -%}
  </tbody>
</table>
</section>


<section class="ds-tokens">
<h2 class="ds-tokens__heading">Animation</h2>
<table class="ds-table"><thead>
<tr>
  <th>Value</th>
  <th>Token title</th>
  <th>Token</th>
</tr></thead>
<tbody>
{% for type in tokens.base.animation -%}
  {% if type[0] == "duration" or type[0] == "easing" %}
  {% for animation in type -%}
    {% for animationType in animation -%}
      {% for property in animationType -%}
        {% if property.value %}
<tr>
  <td><code>{{ property.value }}</code></td>
  <td class="ds-tokens__description">{{ property.description | tokenDescription }}</td>
  <td><code>{{ property.description | tokenName }}</code></td>
</tr>
        {% endif %}
      {% endfor -%}
    {% endfor -%}
  {% endfor -%}
  {% endif %}
{% endfor -%}
</tbody>
</table>
</section>


<section class="ds-tokens">
<h2 class="ds-tokens__heading">Opacity</h2>
<table class="ds-table"><thead>
<tr>
  <th>Value</th>
  <th>Token title</th>
  <th>Token</th>
</tr></thead>
<tbody>
{% for type in tokens.base.opacity -%}
 {% for property in type -%}
   {% if property.value %}
<tr>
  <td>
    <div class="ds-tokens__opacity-example">
      <div class="ds-tokens__opacity-triangles">
        <div class="ds-tokens__triangle" style="opacity: {{ property.value }};{% if property.value == 0 %}display: none;{% endif %}"></div>
      </div>
    <code>{{ property.value }}</code>
    </div>
  </td>
  <td class="ds-tokens__description">{{ property.description | tokenDescription }}</td>
  <td><code>{{ property.description | tokenName }}</code></td>
</tr>
   {% endif %}
 {% endfor -%}
{% endfor -%}
</tbody>
</table>
</section>

