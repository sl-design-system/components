{
  "$schema": "https://raw.githubusercontent.com/lit/lit/main/packages/localize-tools/config.schema.json",
  "sourceLocale": "en",
  "targetLocales": ["nl"],
  "tsConfig": "tsconfig.all.json",
  "output": {
    "mode": "runtime",
    "outputDir": "packages/locales/src",
    "localeCodesModule": "packages/locales/src/locale-codes.ts"
  },
  "interchange": {
    "format": "xliff",
    "xliffDir": "packages/locales/src"
  }
}
