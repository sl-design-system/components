{
  "text": {
    "typeset": {
      "fontFamily": {
        "heading": {
          "$type": "fontFamilies",
          "$value": "The Message"
        },
        "body": {
          "$type": "fontFamilies",
          "$value": "Roboto"
        },
        "icon": {
          "$type": "fontFamilies",
          "$value": "{icon.typeset.fontFamily.classic}",
          "$description": "text.typeset.fontFamily.fontIcon"
        }
      },
      "fontWeight": {
        "regular": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "demibold": {
          "$type": "fontWeights",
          "$value": "Medium"
        }
      }
    }
  },
  "text-new": {
    "body": {
      "sm": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.body.sm}",
          "lineHeight": "{size.lineHeight.200}",
          "fontWeight": "{text.typeset.fontWeight.regular}"
        }
      },
      "md": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.body.md}",
          "lineHeight": "{size.lineHeight.250}",
          "fontWeight": "{text.typeset.fontWeight.regular}"
        }
      },
      "lg": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.body.lg}",
          "lineHeight": "{size.lineHeight.300}",
          "fontWeight": "{text.typeset.fontWeight.regular}"
        }
      }
    },
    "input": {
      "md": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.body.md}",
          "lineHeight": "{size.lineHeight.300}"
        }
      },
      "lg": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.body.md}",
          "lineHeight": "{size.lineHeight.400}"
        }
      }
    },
    "icon": {
      "outline": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{icon.fontFamily.classic}",
          "fontWeight": "{icon.typeset.fontWeight.icon-regular}"
        }
      },
      "solid": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{icon.fontFamily.classic}",
          "fontWeight": "{icon.typeset.fontWeight.icon-solid}"
        }
      }
    },
    "heading": {
      "sm": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.heading.sm}",
          "lineHeight": "{size.lineHeight.250}",
          "fontWeight": "{text.typeset.fontWeight.demibold}"
        }
      },
      "md": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.heading.md}",
          "lineHeight": "{size.lineHeight.250}",
          "fontWeight": "{text.typeset.fontWeight.demibold}"
        }
      },
      "lg": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.heading.lg}",
          "lineHeight": "{size.lineHeight.400}",
          "fontWeight": "{text.typeset.fontWeight.demibold}"
        }
      },
      "xl": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.heading.xl}",
          "lineHeight": "{size.lineHeight.500}",
          "fontWeight": "{text.typeset.fontWeight.demibold}"
        }
      },
      "2xl": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{text.typeset.fontFamily.body}",
          "fontSize": "{size.text.heading.2xl}",
          "lineHeight": "{size.lineHeight.600}",
          "fontWeight": "{text.typeset.fontWeight.demibold}"
        }
      }
    }
  },
  "size": {
    "text": {
      "body": {
        "sm": {
          "$type": "fontSizes",
          "$value": "{size.font.150}"
        },
        "md": {
          "$type": "fontSizes",
          "$value": "{size.font.175}",
          "$description": "default text size for body"
        },
        "lg": {
          "$type": "fontSizes",
          "$value": "{size.font.200}"
        }
      },
      "heading": {
        "sm": {
          "$type": "fontSizes",
          "$value": "{size.font.175}"
        },
        "md": {
          "$type": "fontSizes",
          "$value": "{size.font.200}"
        },
        "lg": {
          "$type": "fontSizes",
          "$value": "{size.font.300}"
        },
        "xl": {
          "$type": "fontSizes",
          "$value": "{size.font.400}"
        },
        "2xl": {
          "$type": "fontSizes",
          "$value": "{size.font.600}"
        }
      }
    },
    "borderRadius": {
      "default": {
        "$type": "borderRadius",
        "$value": "{border.radius.2xs}",
        "$description": "The standard border radius for most components. Use for elements that need subtle rounding without emphasizing their shape."
      },
      "child": {
        "$type": "borderRadius",
        "$value": "{border.radius.2xs} / 2",
        "$description": "The standard border radius for most components. Use for elements that need subtle rounding without emphasizing their shape."
      },
      "full": {
        "$type": "borderRadius",
        "$value": "50rem",
        "$description": "Use for pill-shaped elements like badges, and circular rounding for components with equal width and height, such as icon buttons and avatars."
      },
      "focusRing": {
        "outside": {
          "$type": "borderRadius",
          "$value": "{size.borderRadius.default} + 2",
          "$description": "Use for focusring outside of component"
        },
        "inside": {
          "$type": "borderRadius",
          "$value": "{size.borderRadius.default} - 1",
          "$description": "Use for focusring inside of component when outside focus ring is not possible"
        }
      }
    },
    "borderWidth": {
      "default": {
        "$type": "borderWidth",
        "$value": "{size.borderWidth.subtle}",
        "$description": "Sets the standard border width for UI elements."
      },
      "action": {
        "$type": "borderWidth",
        "$value": "{size.borderWidth.subtle}",
        "$description": "Sets the standard border width for action components."
      },
      "focusRing": {
        "$type": "borderWidth",
        "$value": "{size.borderWidth.bold}"
      }
    },
    "icon": {
      "font": {
        "xs": {
          "$type": "fontSizes",
          "$value": "{size-new.icon.xs}"
        },
        "sm": {
          "$type": "fontSizes",
          "$value": "{size-new.icon.sm}"
        },
        "md": {
          "$type": "fontSizes",
          "$value": "{size-new.icon.md}"
        },
        "lg": {
          "$type": "fontSizes",
          "$value": "{size-new.icon.lg}"
        },
        "xl": {
          "$type": "fontSizes",
          "$value": "{size-new.icon.xl}"
        },
        "2xl": {
          "$type": "fontSizes",
          "$value": "{size-new.icon.2xl}"
        },
        "3xl": {
          "$type": "fontSizes",
          "$value": "{size-new.icon.3xl}"
        },
        "4xl": {
          "$type": "fontSizes",
          "$value": "{size-new.icon.4xl}"
        }
      }
    },
    "outlineWidth": {
      "default": {
        "$type": "borderWidth",
        "$value": "{size.borderWidth.bold}",
        "$description": "Sets the standard outline width for UI elements."
      }
    }
  },
  "icon": {
    "typeset": {
      "fontWeight": {
        "regular": {
          "$type": "fontWeights",
          "$value": "{icon.typeset.fontWeight.icon-regular}"
        },
        "solid": {
          "$type": "fontWeights",
          "$value": "{icon.typeset.fontWeight.icon-solid}"
        }
      }
    }
  },
  "space": {
    "offset": {
      "focused": {
        "$type": "spacing",
        "$value": "{space.025}",
        "$description": "Defines the spacing applied when an element is focused."
      },
      "default": {
        "$type": "spacing",
        "$value": "{space.075}",
        "$description": "Defines the standard spacing offset."
      }
    }
  }
}