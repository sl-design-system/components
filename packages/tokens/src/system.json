				"$value": "{size.800}"
			},
			"1000": {
				"$type": "fontSizes",
				"$value": "{size.1000}"
			}
		},
		"lineHeight": {
			"150": {
				"$type": "lineHeights",
				"$value": "{size.150}"
			},
			"175": {
				"$type": "lineHeights",
				"$value": "{size.175}"
			},
			"200": {
				"$type": "lineHeights",
				"$value": "{size.200}"
			},
			"250": {
				"$type": "lineHeights",
				"$value": "{size.250}"
			},
			"300": {
				"$type": "lineHeights",
				"$value": "{size.300}"
			},
			"400": {
				"$type": "lineHeights",
				"$value": "{size.400}"
			},
			"500": {
				"$type": "lineHeights",
