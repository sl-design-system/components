  "elevation": {
    "shadow": {
      "overflow": {
        "$type": "boxShadow",
        "$value": {
          "x": "0",
          "y": "0",
          "blur": "15",
          "spread": "0",
          "color": "rgba({elevation.surface.shadow}, {opacity.subtlest})",
          "type": "dropShadow"
        },
        "$description": "Use to create a shadow when the content scrolls under other content."
      },
      "overlay": {
        "$type": "boxShadow",
        "$value": [
          {
            "x": "0",
            "y": "6",
            "blur": "12",
            "spread": "-3",
            "color": "rgba({elevation.surface.shadow}, 4%)",
            "type": "dropShadow"
          },
          {
            "x": "0",
            "y": "6",
            "blur": "18",
            "spread": "0",
            "color": "rgba({elevation.surface.shadow}, 12%)",
            "type": "dropShadow"
          }
        ],
        "$description": "Use for the box shadow of elements that sit on top of the UI, such as dialogs, cards, menus and drawers."
      }
    }
  },
  "space-new": {
    "none": {
      "$type": "spacing",
      "$value": "{space.none}"
    },
    "2xs": {
      "$type": "spacing",
      "$value": "{space.025}"
    },
    "xs": {
      "$type": "spacing",
      "$value": "{space.050}"
    },
    "sm": {
      "$type": "spacing",
      "$value": "{space.075}",
      "$description": "Use when components are 'besties'"
    },
    "md": {
      "$type": "spacing",
      "$value": "{space.100}",
      "$description": "Use when components are 'family'"
    },
    "lg": {
      "$type": "spacing",
      "$value": "{space.200}",
      "$description": "Use when components are 'casual friends'"
    },
    "xl": {
      "$type": "spacing",
      "$value": "{space.300}",
      "$description": "Use when components are 'acquaintances'"
    },
    "2xl": {
      "$type": "spacing",
      "$value": "{space.400}",
      "$description": "Use when components are 'strangers'"
    },
    "3xl": {
      "$type": "spacing",
      "$value": "{space.800}"
    },
    "input": {
      "vertical": {
        "$type": "spacing",
        "$value": "{space-new.xs} - {size.010}"
      },
      "horizontal": {
        "$type": "spacing",
        "$value": "{space-new.md} - {size.010}"
      }
    },
    "grid": {
      "vertical": {
        "$type": "spacing",
        "$value": "{space.175}"
      },
      "horizontal": {
        "$type": "spacing",
        "$value": "{space.200}"
      }
    },
    "toggleButton": {
      "nonActive": {
        "sm": {
          "$type": "spacing",
          "$value": "23px"
        },
        "md": {
          "$type": "spacing",
          "$value": "35px"
        },
        "lg": {
          "$type": "spacing",
          "$value": "36px"
        }
      }
    }
