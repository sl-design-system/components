      }
    },
    "borderWidth": {
      "subtle": {
        "$type": "borderWidth",
        "$value": "{size.010}",
        "$description": "A subtle border, applied to UI elements where subtle emphasis is needed."
      },
      "bold": {
        "$type": "borderWidth",
        "$value": "{size.025}",
        "$description": "A bold border, applied to UI elements that need to stand out."
      },
      "heavy": {
        "$type": "borderWidth",
        "$value": "{size.050}",
        "$description": "A heavy border for impactful elements where heavy emphasis is needed."
      },
      "none": {
        "$type": "borderWidth",
        "$value": "{size.none}",
        "$description": "A heavy border for impactful elements where heavy emphasis is needed."
      }
    },
    "outlineWidth": {
      "default": {
        "$type": "borderWidth",
        "$value": "{size.050} - {size.010}",
        "$description": "Defines the default of outlines for focus indicators."
      }
    },
    "outlineOffset": {
      "default": {
        "$type": "spacing",
        "$value": "3px"
      }
    }
  },
  "size-new": {
    "icon": {
