{
  "name": "@sl-design-system/figma-theme-selector",
  "license": "ISC",
  "version": "1.0.0",
  "description": "Figma Plugin to set the theme on a Figma file",
  "main": "code.js",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --ext .ts --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .ts --ignore-pattern node_modules --fix .",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "^0.16.1",
    "@figma/plugin-typings": "^1.109.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^9.21.0",
    "typescript": "^5.4.5"
  }
}
