<section class="ds-tokens">
<h2 class="ds-tokens__heading">Size</h2>
<table class="ds-table"><thead>
<tr>
  <th>Value</th>
  <th>Token title</th>
  <th>Token</th>
</tr></thead>
<tbody>
{% for property in tokens.core.size -%}{% if property[1].value %}
<tr>
  <td>
    <div class="ds-tokens__size-example">
      <div class="ds-tokens__size" style="width: {{ property[1].value }}px; height: {{ property[1].value }}px;"></div>
      <code>{{ property[1].value }}{% if property[1].value != 0 %}px{% endif %}</code>
    </div>
  </td>
  <td class="ds-tokens__description">{{ property[1].description | tokenDescription }}</td>
  <td><code>{{ property[1].description | tokenName }}</code></td>
</tr>
{% endif %}{% endfor -%}
</tbody>
</table>
</section>
